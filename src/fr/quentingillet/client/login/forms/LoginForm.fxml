               <font>
                  <Font size="14.0" />
               </font></TextField>
            <PasswordField layoutX="79.0" layoutY="247.0" prefHeight="26.0" prefWidth="193.0" promptText="Password" fx:id="passwordField">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
         </children>
    </AnchorPane>
  </children>
</VBox>
